<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EMAMUAIAPP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:EMAMUAIAPP"
    Shell.FlyoutBehavior="Flyout"
    Title="EMAMUAIAPP">

   
    <ShellContent
        Title="Inicio"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

   
    <ShellContent
        Title="Clientes"
        ContentTemplate="{DataTemplate local:ClientesPage}"
        Route="ClientesPage" />

    <ShellContent
        Title="Formulario Cliente"
        ContentTemplate="{DataTemplate local:ClienteFormularioPage}"
        Route="ClienteFormularioPage" />

    
    <ShellContent
        Title="Equipos"
        ContentTemplate="{DataTemplate local:EquiposPage}"
        Route="EquiposPage" />

    <ShellContent
        Title="Formulario Equipo"
        ContentTemplate="{DataTemplate local:EquipoFormularioPage}"
        Route="EquipoFormularioPage" />
</Shell>

